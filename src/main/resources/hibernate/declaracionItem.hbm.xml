<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="regiplast.domain.entity">
	<class name="DeclaracionItem" table="declaracion_item">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <many-to-one name="proveedor" class="regiplast.domain.entity.Proveedor" column="id_proveedor" not-null="true"/>
        <many-to-one name="tipoDeclaracion" class="regiplast.domain.entity.TipoBase" column="id_tipo_declaracion" not-null="true"/>
        <many-to-one name="tipoBien" class="regiplast.domain.entity.TipoBase" column="id_tipo_bien" not-null="true"/>
        <many-to-one name="tipoResina" class="regiplast.domain.entity.TipoBase" column="id_tipo_resina" not-null="true"/>
        <many-to-one name="tipoUso" class="regiplast.domain.entity.TipoBase" column="id_tipo_uso" not-null="true"/>
        <many-to-one name="formaValoracion" class="regiplast.domain.entity.TipoBase" column="id_forma_valoracion" not-null="true"/>
        <many-to-one name="origen" class="regiplast.domain.entity.Origen" column="id_origen_bien" not-null="true"/>
        <many-to-one name="destino" class="regiplast.domain.entity.Destino" column="id_destino_bien" not-null="true"/>
        
        <property name="biodegradable" column="biodegradable"/>
        <property name="porcentajeReciclado" column="porcentaje_reciclado"/>
        <property name="cantidad" column="cantidad"/>
        <property name="peso" column="peso"/>
        <property name="mes" column="mes"/>
    </class> 
</hibernate-mapping>